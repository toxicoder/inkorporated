{
    "mcpServers": {
        "cline-community": {
            "autoApprove": [
                "preview_cline_issue"
            ],
            "timeout": 10,
            "command": "node",
            "args": [
                "/path/to/cline-community/build/index.js"
            ],
            "transportType": "stdio",
            "env": {
                "GH_TOKEN": "${GH_TOKEN}"
            },
            "disabled": true
        },
        "github": {
            "command": "docker",
            "args": [
                "run",
                "-i",
                "--rm",
                "ghcr.io/github/github-mcp-server"
            ],
            "env": {
                "GITHUB_PERSONAL_ACCESS_TOKEN": "${GITHUB_PERSONAL_ACCESS_TOKEN}"
            },
            "disabled": true
        },
        "puppeteer": {
            "command": "npx",
            "args": [
                "-y",
                "@modelcontextprotocol/server-puppeteer"
            ],
            "disabled": true
        },
        "filesystem": {
            "command": "node",
            "args": [
                "/path/to/filesystem-mcp-server/dist/index.js"
            ],
            "env": {
                "MCP_LOG_LEVEL": "${MCP_LOG_LEVEL}",
                "FS_BASE_DIRECTORY": "${FS_BASE_DIRECTORY}",
                "MCP_TRANSPORT_TYPE": "${MCP_TRANSPORT_TYPE}"
            },
            "disabled": true,
            "autoApprove": []
        },
        "git-tools": {
            "type": "stdio",
            "command": "npx",
            "args": [
                "@cyanheads/git-mcp-server@latest"
            ],
            "env": {
                "MCP_TRANSPORT_TYPE": "${MCP_TRANSPORT_TYPE}",
                "MCP_LOG_LEVEL": "${MCP_LOG_LEVEL}",
                "GIT_BASE_DIR": "${GIT_BASE_DIR}",
                "LOGS_DIR": "${LOGS_DIR}",
                "GIT_USERNAME": "${GIT_USERNAME}",
                "GIT_EMAIL": "${GIT_EMAIL}",
                "GIT_SIGN_COMMITS": "${GIT_SIGN_COMMITS}"
            },
            "disabled": true
        },
        "perplexity": {
            "command": "npx",
            "args": [
                "-y",
                "@perplexity-ai/mcp-server"
            ],
            "env": {
                "PERPLEXITY_API_KEY": "${PERPLEXITY_API_KEY}"
            },
            "disabled": true,
            "alwaysAllow": []
        },
        "tavily": {
            "command": "npx",
            "args": [
                "-y",
                "tavily-mcp"
            ],
            "env": {
                "TAVILY_API_KEY": "${TAVILY_API_KEY}"
            },
            "disabled": true
        },
        "mongodb": {
            "command": "npx",
            "args": [
                "-y",
                "mcp-mongo-server",
                "${MCP_MONGODB_URI}"
            ],
            "env": {
                "MCP_MONGODB_URI": "${MCP_MONGODB_URI}",
                "MCP_MONGODB_READONLY": "${MCP_MONGODB_READONLY}"
            },
            "disabled": true
        },
        "redis": {
            "type": "stdio",
            "command": "uvx",
            "args": [
                "--from",
                "redis-mcp-server@latest",
                "redis-mcp-server",
                "--url",
                "redis://localhost:6379/0"
            ],
            "env": {
                "REDIS_HOST": "${REDIS_HOST}",
                "REDIS_PORT": "${REDIS_PORT}",
                "REDIS_PWD": "${REDIS_PWD}"
            },
            "disabled": true
        },
        "slack": {
            "command": "go",
            "args": [
                "run",
                "/path/to/slack-mcp-server/mcp/mcp-server.go",
                "--transport",
                "stdio"
            ],
            "env": {
                "SLACK_MCP_XOXC_TOKEN": "${SLACK_MCP_XOXC_TOKEN}",
                "SLACK_MCP_LOG_LEVEL": "${MCP_LOG_LEVEL}"
            },
            "disabled": true
        },
        "docker-mcp": {
            "command": "uvx",
            "args": [
                "docker-mcp"
            ],
            "disabled": true
        },
        "postgres": {
            "command": "npx",
            "args": [
                "-y",
                "@modelcontextprotocol/server-postgres",
                "postgresql://${PG_USER}:${PG_PASSWORD}@${PG_HOST}:${PG_PORT}/${PG_DATABASE}"
            ],
            "env": {
                "PG_HOST": "${PG_HOST}",
                "PG_PORT": "${PG_PORT}",
                "PG_USER": "${PG_USER}",
                "PG_PASSWORD": "${PG_PASSWORD}",
                "PG_DATABASE": "${PG_DATABASE}"
            },
            "disabled": true
        },
        "sqlite": {
            "command": "npx",
            "args": [
                "-y",
                "@modelcontextprotocol/server-sqlite",
                "${SQLITE_DB_PATH}"
            ],
            "env": {
                "SQLITE_DB_PATH": "${SQLITE_DB_PATH}"
            },
            "disabled": true
        },
        "gdrive": {
            "command": "npx",
            "args": [
                "-y",
                "@modelcontextprotocol/server-gdrive"
            ],
            "env": {
                "GDRIVE_CREDENTIALS": "${GDRIVE_CREDENTIALS}",
                "GDRIVE_ROOT_FOLDER_ID": "${GDRIVE_ROOT_FOLDER_ID}"
            },
            "disabled": true
        },
        "git-official": {
            "type": "stdio",
            "command": "uvx",
            "args": [
                "mcp-server-git",
                "--repository",
                "/path/to/git/repo"
            ],
            "disabled": true
        },
        "gitlab": {
            "command": "npx",
            "args": [
                "-y",
                "@modelcontextprotocol/server-gitlab"
            ],
            "env": {
                "GITLAB_URL": "${GITLAB_URL}",
                "GITLAB_TOKEN": "${GITLAB_TOKEN}",
                "GITLAB_PROJECT_ID": "${GITLAB_PROJECT_ID}"
            },
            "disabled": true
        },
        "sentry": {
            "command": "npx",
            "args": [
                "-y",
                "@modelcontextprotocol/server-sentry"
            ],
            "env": {
                "SENTRY_URL": "${SENTRY_URL}",
                "SENTRY_AUTH_TOKEN": "${SENTRY_AUTH_TOKEN}",
                "SENTRY_PROJECT": "${SENTRY_PROJECT}"
            },
            "disabled": true
        },
        "context7": {
            "command": "npx",
            "args": [
                "mcp-cache",
                "npx",
                "-y",
                "@upstash/context7-mcp@latest",
                "--api-key",
                "${CONTEXT7_API_KEY}"
            ],
            "env": {
                "MCP_CACHE_CACHE_DIR": "/home/vscode/.cline/cache",
                "MCP_CACHE_MAX_TOKENS": "25000",
                "MCP_CACHE_TTL": "7200",
                "MCP_CACHE_ENABLE_INDEXING": "true",
                "MCP_CACHE_COMPRESSION": "true",
                "CONTEXT7_API_KEY": "${CONTEXT7_API_KEY}"
            },
            "disabled": true
        },
        "ollama": {
            "command": "npx",
            "args": [
                "-y",
                "@modelcontextprotocol/server-ollama"
            ],
            "env": {
                "OLLAMA_API_BASE_URL": "${OLLAMA_API_BASE_URL}",
                "OLLAMA_MODEL": "${OLLAMA_MODEL}"
            },
            "disabled": true
        },
        "open-webui": {
            "command": "npx",
            "args": [
                "-y",
                "@modelcontextprotocol/server-open-webui"
            ],
            "env": {
                "OPEN_WEBUI_API_BASE_URL": "${OPEN_WEBUI_API_BASE_URL}",
                "OPEN_WEBUI_API_KEY": "${OPEN_WEBUI_API_KEY}"
            },
            "disabled": true
        },
        "vector-search": {
            "command": "npx",
            "args": [
                "-y",
                "@modelcontextprotocol/server-vector-search"
            ],
            "env": {
                "VECTOR_SEARCH_EMBEDDING_MODEL": "${VECTOR_SEARCH_EMBEDDING_MODEL}",
                "VECTOR_SEARCH_INDEX_PATH": "${VECTOR_SEARCH_INDEX_PATH}"
            },
            "disabled": true
        }
    }
}